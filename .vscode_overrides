#!/usr/bin/env bash

alias code="clear && code"
alias srit="clear && source \$HOME/.bashrc"
alias yarn="clear && yarn"
unalias gcam && alias gcam='clear && git commit -a -m'
unalias gcmsg && alias gcmsg='clear && git commit -m'
unalias gp && alias gp='clear && git push'

